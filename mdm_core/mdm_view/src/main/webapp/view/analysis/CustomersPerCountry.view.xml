<mvc:View height="100%"
	controllerName="uni.mannheim.mdm.controller.analysis.CustomersPerCountry"
	xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:vbm="sap.ui.vbm" displayBlock="true">

	<VBox class="mdmScreenTitle">
		<Text class="mdmTitle" text="Customers per Country" />
	</VBox>

	<vbm:AnalyticMap class="mdmAnalysisMap" regionClick="onRegionClick">
		<vbm:regions>
			<vbm:Region code="DE" color="rgba(184,225,245,1.0)" />
			<vbm:Region code="US" color="rgba(5,71,102,1.0)" />
			<vbm:Region code="CN" color="rgba(0,125,192,1.0)" />
		</vbm:regions>
	</vbm:AnalyticMap>

</mvc:View>