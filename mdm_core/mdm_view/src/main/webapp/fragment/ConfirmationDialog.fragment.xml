<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog id="confirmationDialog"
		title="{dialog>/title}"
		typ="{dialog>/type}"
		state="{dialog>/state}">
		<content>
			<Text class="mdmConfirmationDialogTxt" text="{dialog>/text}"/>
		</content>
		<beginButton>
			<Button text="OK" press="onDialogConfirmed"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onDialogCanceled"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>
